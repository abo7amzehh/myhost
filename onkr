echo "Start"
if [ -e "/sdcard/Android/data/libtersafe.so" ];
then
echo 
else
curl -o /sdcard/Android/data/libtersafe.so https://raw.githubusercontent.com/abo7amzehh/myhost/master/libtersafe.so
wget -O /sdcard/Android/data/libtersafe.so https://raw.githubusercontent.com/abo7amzehh/myhost/master/libtersafe.so
fi
chattr -R -i /data/media/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Config/Android/UserCustom.ini
chattr -R -i /data/media/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Config/Android/UserSettings.ini
am force-stop com.irugratzkuc
am force-stop com.pubg.krmobile
am force-stop com.iqhbamtwcp.fqfuukz
rm -rf /data/data/com.pubg.krmobile/lib/libtersafe.so
echo
cp /storage/emulated/0/Android/data/libtersafe.so /data/data/com.pubg.krmobile/lib/
echo
chmod -R 755 /data/data/com.pubg.krmobile/lib/libtersafe.so
chmod -R 2000 /data/data/com.pubg.krmobile/files
sleep 0.5
chmod 775 /data/data/com.pubg.krmobile/files/.iii
rm -rf /data/data/com.pubg.krmobile/app_crashrecord
touch /data/data/com.pubg.krmobile/app_crashrecord
cp -R /storage/emulated/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.18.0.12624.pak /storage/emulated/0/Android/data/
sleep 0.5
rm -rf /storage/emulated/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.18.0.12624.pak
dd if=/dev/urandom of=/storage/emulated/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.18.0.12600.pak bs=23622 count=100 &> /dev/null
dd if=/dev/urandom of=/storage/emulated/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.18.0.12619.pak bs=23622 count=100 &> /dev/null
dd if=/dev/urandom of=/storage/emulated/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.18.0.12625.pak bs=23622 count=100 &> /dev/null
dd if=/dev/urandom of=/storage/emulated/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.18.0.12624.pak bs=23622 count=100 &> /dev/null
chmod 550 /data/media/0/Android/data/com.pubg.krmobile/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks
sleep 2
am start -a android.intent.action.MAIN -n com.irugratzkuc/.ActivityMain
am start -a android.intent.action.MAIN -n com.iqhbamtwcp.fqfuukz/.ActivityMain
sleep 3
am start -n com.pubg.krmobile/com.epicgames.ue4.SplashActivity
sleep 4
chown 1000:1000 /data/data/com.pubg.krmobile/lib/*
rm -rf /data/cache/magisk.log
rm -rf /storage/emulated/0/.backups
rm -rf /storage/emulated/0/flywheel
rm -rf /storage/emulated/0/MidasOversea
rm -rf /storage/emulated/0/tencent
